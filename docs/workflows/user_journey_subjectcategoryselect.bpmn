<?xml version="1.0" ?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_user_journey_subjectcategoryselect" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="user_journey_subjectcategoryselect" name="User Journey - SubjectCategorySelect" isExecutable="false">
    <bpmn:startEvent id="start_0" name="User Starts Journey"/>
    <bpmn:userTask id="user_task_1" name="Click  =&amp;gt; field.onChange(field.value === subjectKey ? &amp;apos;&amp;apos; : subjectKey)" actor="user"/>
    <bpmn:userTask id="user_task_2" name="Change (e) =&amp;gt; setSearchTerm(e.target.value)" actor="user"/>
    <bpmn:serviceTask id="system_task_3" name="Update SearchTerm" actor="system"/>
    <bpmn:endEvent id="end_4" name="Journey Complete"/>
    <bpmn:sequenceFlow id="flow_0" sourceRef="start_0" targetRef="user_task_1"/>
    <bpmn:sequenceFlow id="flow_1" sourceRef="user_task_1" targetRef="user_task_2"/>
    <bpmn:sequenceFlow id="flow_2" sourceRef="user_task_2" targetRef="system_task_3"/>
    <bpmn:sequenceFlow id="flow_3" sourceRef="system_task_3" targetRef="end_4"/>
  </bpmn:process>
</bpmn:definitions>
