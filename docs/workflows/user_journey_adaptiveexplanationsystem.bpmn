<?xml version="1.0" ?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_user_journey_adaptiveexplanationsystem" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="user_journey_adaptiveexplanationsystem" name="User Journey - AdaptiveExplanationSystem" isExecutable="false">
    <bpmn:startEvent id="start_0" name="User Starts Journey"/>
    <bpmn:userTask id="user_task_1" name="Click  =&amp;gt; {
                // Could open advanced settings dialog
              " actor="user"/>
    <bpmn:userTask id="user_task_2" name="Change (e) =&amp;gt; setAutoAdapt(e.target.checked)" actor="user"/>
    <bpmn:userTask id="user_task_3" name="Change (e) =&amp;gt; ConfigChange(&amp;apos;userRole&amp;apos;, e.target.value)" actor="user"/>
    <bpmn:userTask id="user_task_4" name="Change (_, value) =&amp;gt; ConfigChange(&amp;apos;complexity&amp;apos;, value)" actor="user"/>
    <bpmn:userTask id="user_task_5" name="Change (e) =&amp;gt; ConfigChange(&amp;apos;explanationStyle&amp;apos;, e.target.value)" actor="user"/>
    <bpmn:serviceTask id="system_task_6" name="Update Config" actor="system"/>
    <bpmn:serviceTask id="system_task_7" name="Update AdaptiveContent" actor="system"/>
    <bpmn:serviceTask id="system_task_8" name="Update Config" actor="system"/>
    <bpmn:serviceTask id="system_task_9" name="Update AutoAdapt" actor="system"/>
    <bpmn:endEvent id="end_10" name="Journey Complete"/>
    <bpmn:sequenceFlow id="flow_0" sourceRef="start_0" targetRef="user_task_1"/>
    <bpmn:sequenceFlow id="flow_1" sourceRef="user_task_1" targetRef="user_task_2"/>
    <bpmn:sequenceFlow id="flow_2" sourceRef="user_task_2" targetRef="user_task_3"/>
    <bpmn:sequenceFlow id="flow_3" sourceRef="user_task_3" targetRef="user_task_4"/>
    <bpmn:sequenceFlow id="flow_4" sourceRef="user_task_4" targetRef="user_task_5"/>
    <bpmn:sequenceFlow id="flow_5" sourceRef="user_task_5" targetRef="system_task_6"/>
    <bpmn:sequenceFlow id="flow_6" sourceRef="system_task_6" targetRef="system_task_7"/>
    <bpmn:sequenceFlow id="flow_7" sourceRef="system_task_7" targetRef="system_task_8"/>
    <bpmn:sequenceFlow id="flow_8" sourceRef="system_task_8" targetRef="system_task_9"/>
    <bpmn:sequenceFlow id="flow_9" sourceRef="system_task_9" targetRef="end_10"/>
  </bpmn:process>
</bpmn:definitions>
